<!DOCTYPE html>
<html>
  <% include ../partials/head %>
  <body class="cyan">
    <div id="login-page" class="row">
    <div class="col s12 z-depth-4 card-panel">
      <form class="login-form" method="POST" action="/register">
        <div class="row">
          <div class="input-field col s12 center">
            <img src="http://demo.geekslabs.com/materialize-v1.0/images/login-logo.png" alt="" class="circle responsive-img valign profile-image-login">
            <p class="center login-form-text">Register</p>
          </div>
        </div>
        <div class="row margin">
          <div class="input-field col s12">
            <i class="material-icons prefix">account_circle</i>
            <input id="username" value="s" name="username" type="text" class="validate">
            <label for="username">User Name</label>          
          </div>
        </div>
        <div class="row margin">
          <div class="input-field col s12">
            <i class="material-icons prefix">lock_outline</i>
            <input name="password" id="password" value="s" type="password">
            <label for="password">Password</label>
          </div>
        </div>
        <div class="row margin">
          <div class="input-field col s12">
            <i class="material-icons prefix">account_circle</i>
            <input id="name" name="name" value="s" type="text" class="validate">
            <label for="name">Full Name</label>          
          </div>
        </div>
        <div class="row margin">
          <div class="input-field col s12">
            <i class="material-icons prefix">account_balance</i>
            <input id="schoolName" name="schoolName" value="s" type="text" class="validate">
            <label for="schoolName">School Name</label>          
          </div>
        </div>
        <div class="row margin">
          <div class="input-field col s12">
            <i class="material-icons prefix">link</i>
            <input id="photo" name="photo" value="s" type="text" class="validate">
            <label for="photo">Photo</label>          
          </div>
        </div>
        <div class="row margin">
          <div class="input-field col s12">
            <i class="material-icons prefix">link</i>
            <input id="schoolAddress" name="schoolAddress" value="American High School, Fremont Boulevard, Fremont, CA" type="text" class="validate">
            <label for="schoolAddress">School Address</label>          
          </div>
        </div>
        <div class="row margin">
          <div class="input-field col s12">
            <i class="material-icons prefix">link</i>
            <input id="homeAddress" name="homeAddress" value="3107 Salisbury Court, Fremont, CA" type="text" class="validate">
            <label for="homeAddress">Home Address</label>          
          </div>
        </div>
        <div class="row margin">
          <div class="input-field col s12">
            <i class="material-icons prefix">account_circle</i>
            <input id="email" name="email" type="email" value="s@s" class="validate">
            <label for="email">Email</label>          
          </div>
        </div>
        <div class="row margin">
          <div class="input-field col s12">
            <i class="material-icons prefix">account_circle</i>
            <input id="phone" name="phone" type="tel" value="1" class="validate">
            <label for="phone">Phone</label>          
          </div>
        </div>
        <div class="row">
          <button class="btn waves-effect waves-light input-field col s12" type="submit" name="action">Register</button>        
        </div>
      </form>
    </div>
  </div>
    <% include ../partials/footer %> 
  </body>
</html>
