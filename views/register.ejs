<!DOCTYPE html>
<html>
  <% include ../partials/head %>
  <link href="/stylesheets/login.css" type="text/css" rel="stylesheet">
  <body class="red lighten-3">
    <div id="login-page" class="row">
    <div class="col s12 z-depth-4 card-panel">
      <form class="login-form" method="POST" action="/register">
        <div class="row">
          <div class="input-field col s12 center">
            <h4 class="center login-form-text">Register</h4>
          </div>
        </div>
        <div class="row margin">
          <div class="input-field col s12">
            <i class="material-icons prefix">account_circle</i>
            <input id="username" value="soham" name="username" type="text" class="validate">
            <label for="username">User Name</label>          
          </div>
        </div>
        <div class="row margin">
          <div class="input-field col s12">
            <i class="material-icons prefix">lock_outline</i>
            <input name="password" id="password" value="soham" type="password">
            <label for="password">Password</label>
          </div>
        </div>
        <div class="row margin">
          <div class="input-field col s12">
            <i class="material-icons prefix">account_circle</i>
            <input id="name" name="name" value="Soham Ashodiya" type="text" class="validate">
            <label for="name">Full Name</label>          
          </div>
        </div>
        <div class="row margin">
          <div class="input-field col s12">
            <i class="material-icons prefix">account_balance</i>
            <input id="schoolName" name="schoolName" value="American High School" type="text" class="validate">
            <label for="schoolName">School Name</label>          
          </div>
        </div>
        <div class="row margin">
          <div class="input-field col s12">
            <i class="material-icons prefix">insert_photo</i>
            <input id="photo" name="photo" value="data:image/jpeg;base64," type="text" class="validate">
            <label for="photo">Photo</label>          
          </div>
        </div>
        <div class="row margin">
          <div class="input-field col s12">
            <i class="material-icons prefix">school</i>
            <input id="schoolAddress" name="schoolAddress" value="American High School, Fremont Boulevard, Fremont, CA" type="text" class="validate">
            <label for="schoolAddress">School Address</label>          
          </div>
        </div>
        <div class="row margin">
          <div class="input-field col s12">
            <i class="material-icons prefix">home</i>
            <input id="homeAddress" name="homeAddress" value="3107 Salisbury Court, Fremont, CA" type="text" class="validate">
            <label for="homeAddress">Home Address</label>          
          </div>
        </div>
        <div class="row margin">
          <div class="input-field col s12">
            <i class="material-icons prefix">email</i>
            <input id="email" name="email" type="email" value="sohamashodiya@gmail.com" class="validate">
            <label for="email">Email</label>          
          </div>
        </div>
        <div class="row margin">
          <div class="input-field col s12">
            <i class="material-icons prefix">phone</i>
            <input id="phone" name="phone" type="tel" value="5103243100" class="validate">
            <label for="phone">Phone</label>          
          </div>
        </div>
        <div class="row">
          <button class="btn waves-effect waves-light input-field col s12" type="submit" name="action">Register</button>        
        </div>
      </form>
    </div>
  </div>
    <% include ../partials/footer %> 
  </body>
</html>
